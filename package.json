{
  "name": "ng-webpack-seed",
  "version": "1.0.0",
  "description": "Angular 1.5 Webpack seed with ES6 support ",
  "main": "index.js",
  "scripts": {
    "serve": "node node_modules/.bin/webpack-dev-server --hot --inline --progress --colors",
    "build": "node node_modules/.bin/rimraf dist && NODE_ENV=production webpack -p --config webpack.prod.config.js --progress",
    "serve:dist": "node node_modules/.bin/http-server ./dist -p 3000",
    "start": "npm run serve"
  },
  "keywords": [
    "angular",
    "webpack",
    "angular-webpack",
    "es6"
  ],
  "author": "Chyngyz Arystan uulu",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "copy-webpack-plugin": "^2.1.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.16.0",
    "http-server": "^0.9.0",
    "ng-annotate-loader": "^0.1.0",
    "node-sass": "^3.4.2",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.4.3",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.15",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "angular": "^1.5.6",
    "jquery": "^2.2.3",
    "ngcomponentrouter": "^2.1.0"
  }
}
